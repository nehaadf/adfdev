{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-neha25-datafactory"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorage2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage2'"
		},
		"AzureBlobStorage3_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage3'"
		},
		"BlobStorageOutput_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorageOutput'"
		},
		"OnPremServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'OnPremServer'"
		},
		"SQLDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQLDB'"
		},
		"adfdatalakegen25_linkedservice_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adfdatalakegen25_linkedservice'"
		},
		"adflabneha25storageacc_linkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'adflabneha25storageacc_linkedservice'"
		},
		"adflabstorageaccount_linkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'adflabstorageaccount_linkedservice'"
		},
		"taxisql_linkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'taxisql_linkedservice'"
		},
		"unzip_AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'unzip_AzureDataLakeStorage1'"
		},
		"OnPremServer_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\adflabvm\\sampledata"
		},
		"OnPremServer_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "developer"
		},
		"adfdatalakegen25_linkedservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfdatalakegen25.dfs.core.windows.net/"
		},
		"unzip_AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfdatalakegen25.dfs.core.windows.net/"
		},
		"Trigger_8sj_properties_pipeline_parameters_windowStart": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"Trigger_8sj_properties_pipeline_parameters_windowEnd": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"Trigger_ci3_properties_pipeline_parameters_windowStart": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"Trigger_ci3_properties_pipeline_parameters_windowEnd": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-neha25-datafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage2_connectionString": {
						"value": "[parameters('AzureBlobStorage2_connectionString')]"
					},
					"AzureBlobStorage3_connectionString": {
						"value": "[parameters('AzureBlobStorage3_connectionString')]"
					},
					"SQLDB_connectionString": {
						"value": "[parameters('SQLDB_connectionString')]"
					},
					"adfdatalakegen25_linkedservice_accountKey": {
						"value": "[parameters('adfdatalakegen25_linkedservice_accountKey')]"
					},
					"adflabneha25storageacc_linkedservice_connectionString": {
						"value": "[parameters('adflabneha25storageacc_linkedservice_connectionString')]"
					},
					"adflabstorageaccount_linkedservice_connectionString": {
						"value": "[parameters('adflabstorageaccount_linkedservice_connectionString')]"
					},
					"taxisql_linkedservice_connectionString": {
						"value": "[parameters('taxisql_linkedservice_connectionString')]"
					},
					"unzip_AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('unzip_AzureDataLakeStorage1_accountKey')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"BlobStorageOutput_connectionString": {
						"value": "[parameters('BlobStorageOutput_connectionString')]"
					},
					"OnPremServer_password": {
						"value": "[parameters('OnPremServer_password')]"
					},
					"adfdatalakegen25_linkedservice_properties_typeProperties_url": {
						"value": "[parameters('adfdatalakegen25_linkedservice_properties_typeProperties_url')]"
					},
					"unzip_AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('unzip_AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"OnPremServer_properties_typeProperties_host": {
						"value": "[parameters('OnPremServer_properties_typeProperties_host')]"
					},
					"OnPremServer_properties_typeProperties_userId": {
						"value": "[parameters('OnPremServer_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-neha25-datafactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Trigger_8sj_properties_pipeline_parameters_windowStart": {
						"value": "[parameters('Trigger_8sj_properties_pipeline_parameters_windowStart')]"
					},
					"Trigger_8sj_properties_pipeline_parameters_windowEnd": {
						"value": "[parameters('Trigger_8sj_properties_pipeline_parameters_windowEnd')]"
					},
					"Trigger_ci3_properties_pipeline_parameters_windowStart": {
						"value": "[parameters('Trigger_ci3_properties_pipeline_parameters_windowStart')]"
					},
					"Trigger_ci3_properties_pipeline_parameters_windowEnd": {
						"value": "[parameters('Trigger_ci3_properties_pipeline_parameters_windowEnd')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-neha25-datafactory_ArmTemplate_0')]"
			]
		}
	]
}